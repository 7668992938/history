        pagePathLevel2 = pagePathLevel2.split('?')[0];
    }

    if(pagePathLevel2 && pagePathLevel2.indexOf('#')!==-1) {
        pagePathLevel2 = pagePathLevel2.split('#')[0];
    }

    category = pagePathLevel2;

    if (!category) {
        return "home"; //default
