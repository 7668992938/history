<script>

    window.jQueryWatcher = setInterval(function(){
        if(window.jQuery){
            $('#modal-otp').on('hidden.bs.modal', function (e) {
                $('#otpSuccessMsg').text(staticParams.leadSuccessMessage);
                $('#otp-success').modal();
            });
            window.clearInterval(jQueryWatcher);
        }
    }, 500);

    staticParams.otpValue = "";
    function changeInput(target){
        if (target.nextElementSibling && target.value) {
            target.nextElementSibling.focus();
        }
        var otpValues = "";
        var inputEls = document.querySelectorAll('.content-opt input');
        for (var i = 0; i < inputEls.length; i++) {
            otpValues += inputEls[i].value;
        }
        staticParams.otpValue = otpValues;
    }

    function resendHandler(){
        $('#errorOtp').text('Sending an OTP !!!').show();
        $.ajax({
            url: '/api/1.0/Leads/' + staticParams.message + '/newOtp/true',
            method: 'PUT',
            data: {
                contact: staticParams.contact
            },
            success: function(data){
                if (data.status == 200 && data.message) {
                    $('#errorOtp').text('A new OTP has been sent !!!');
                }
            },
            error: function(err, message){
                $('#errorOtp').text('You have entered wrong OTP !!!').show();
                setTimeout(function(){
                    $('#errorOtp').text('');
                },1000);
            }
        })
    }

    function verifyOTP(){
        if (!staticParams.otpValue || staticParams.otpValue.length != 4) {
            $('#errorOtp').text('Please enter a valid OTP !!!');
            setTimeout(function(){
                $('#errorOtp').text('');
            },1000);
            return;
        }

        $('#form-loader').show();
        $.ajax({
            url: '/api/1.0/Leads/' + staticParams.message + '/otp/true',
            method: 'PUT',
            data: {
                contact: staticParams.contact,
                otp: staticParams.otpValue
            },
            success: function(data){
                if (data.status == 200 && data.message) {
                    $('#modal-otp').modal('hide');
                    $('#otpSuccessMsg').text(staticParams.leadSuccessMessage);
                    $('#otp-success').modal();
                }
            },
            error: function(err, message){
                $('#errorOtp').text('You have entered wrong OTP !!!').show();
                setTimeout(function(){
                    $('#errorOtp').text('');
                },1000);
            }
        })
    }
</script>
